<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:smw="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_four"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <com.customview.view.CustomVolumeControlBar
        android:layout_width="300dp"
        android:layout_height="300dp"
        smw:firstColor="@color/colorAccent"
        smw:secondColor="@color/colorPrimary"
        smw:background="@mipmap/ic_launcher"
        smw:circleWidth="8dp"
        smw:dotCount="10"
        smw:splitSize="12"
        />

</RelativeLayout>
